<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">



	<bean id="employee" class="com.test.Employee" autowire="autodetect">
		<constructor-arg value="aadesh"></constructor-arg>
		<constructor-arg value="21"></constructor-arg>
	</bean>


	<bean id="address" class="com.test.Address">
		<constructor-arg value="J.M Road"></constructor-arg>
		<constructor-arg value="pune"></constructor-arg>
		<constructor-arg value="maharashtra"></constructor-arg>
		<constructor-arg value="india"></constructor-arg>
	</bean>





</beans>
